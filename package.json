{
  "name": "collecteur-mesinfos",
  "version": "0.8.0",
  "description": "Le collecteur de données pour le projet MesInfos vous permet de récupérer automatiquement vos données détenues par les entreprises.",
  "main": "server.coffee",
  "dependencies": {
    "jugglingdb": "0.2.2",
    "jugglingdb-cozy-adapter": "0.4.0",
    "express": "3.4.4",
    "async": "0.2.9",
    "cozy-realtime-adapter": "0.12.0",
    "cozy-notifications-helper": "0.0.3",
    "request-json": "0.3.0",
    "oauth": "0.9.10",
    "jade": "0.34.1",
    "dateformat": "1.0.6-1.2.3",
    "mashape-oauth": "0.1.5",
    "request": "2.26.0"
  },
  "devDependencies": {
    "chai": "~1.5.0",
    "mocha": "~1.9.0",
    "sinon": "~1.6.0",
    "coffee-script": "*"
  },
  "scripts": {
    "test": "mocha server/_specs/ --reporter spec --compilers coffee:coffee-script --colors",
    "start": "coffee server.coffee"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsilvestre/cozy-data-integrator.git"
  },
  "author": "Cozycloud",
  "readmeFilename": "README.md",
  "cozy-displayName": "Collecteur de données",
  "cozy-permissions": {
    "All": {
      "description": "L'application a besoin d'accéder à tous les doctypes pour intégrer les données des entreprises partenares du projet MesInfos."
    }
  }
}
